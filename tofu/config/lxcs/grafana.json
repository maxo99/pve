{
  "container_name": "grafana-monitoring",
  "container_id": 105,
  "node_name": "pve-01",
  "description": "Container LXC Grafana Monitoring",
  "tags": [
    "monitoring",
    "grafana",
    "lxc"
  ],
  "memory": 512,
  "cores": 1,
  "disk_size": 4,
  "network_bridge": "vmbr0",
